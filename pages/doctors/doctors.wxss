/* pages/doctors/doctors.wxss */
#ct{
  width: 100%;
  height: 100vh;
  background-color: rgba(245, 245, 245,0.9); 
}
.scr{
  background-color: #f5f2f2;
  height: 80rpx;
  width: auto;
  /* margin-top: 2rpx; */
  overflow: hidden;
  border-bottom: 5rpx solid #eeeeee;
  /* position: fixed; */
  font-size: 16px;
  /* top: 0;
  left: 0; */
  /* z-index: 99; */
  line-height: 80rpx;
  white-space: nowrap;
}
.NumList{
  width: 98%;
  margin: 0 auto;
}
.navInfo{
  width: 100%;
  display: inline-flex;
  justify-content: space-between;
  padding: 15rpx 0;
  border-radius: 10rpx;
  border-bottom: 5rpx dashed grey;
  background-color: rgba(255, 255, 255,0.8);
}
.nav .active{
  color: rgb(0, 173, 241);
  text-decoration: steelblue;
  font-weight: 500;
}
.Dimg{
  width:230rpx;
  margin-left: 15rpx;

}
.Dimg image{
  width: 100%;
  height: 200rpx;
}
.navInfo .docInfo{
  margin-left: 15rpx;
  display: inline-flex;
  flex-direction: column;
  /* overflow: hidden;
  text-overflow: ellipsis; */
  width: 450rpx;
}
.state{
  padding: 50rpx 0;
  margin-right: 15rpx;
  /* float: right; */
  writing-mode: vertical-lr;
}
.state .open{
  color: green;
  font-size: small;
}
.state .close{
  color: red;
  font-size: small;
}
.tip{
  width: 100%;
  height: 560px;
  text-align: center;
  padding-top: 200px;
}